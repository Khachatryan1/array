#include <iostream>
using namespace std;
void func(int arr[],const int length)
{
    int max = 0;
    for (int i = 0; i < length; i++) {
        if (max < arr[i]) {
            max = arr[i];
        }
    }
    cout << max << "\t";
}
int main() 
{
    const int length = 10;
    int arr[length] = { 1,3,6,9,8,5,23,25,63,32 };
    func(arr, length);
    
    return 0;
}
